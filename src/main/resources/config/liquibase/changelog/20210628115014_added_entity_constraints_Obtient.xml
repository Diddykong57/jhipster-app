        <addForeignKeyConstraint baseColumnNames="etudiant_id"
                                 baseTableName="obtient"
                                 constraintName="fk_obtient__etudiant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="etudiant"/>
    </changeSet>
</databaseChangeLog>
